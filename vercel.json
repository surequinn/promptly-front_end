{
  "version": 2,
  "public": true,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "build": {
    "env": {
      "EXPO_PUBLIC_API_URL": "https://promptly-back-mp5guwmbm-quinns-projects-3ee04bc1.vercel.app"
    }
  },
  "outputDirectory": "dist"
}
